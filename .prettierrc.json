{
  "trailingComma": "es5",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "tailwindConfig": "./client/tailwind.config.js",
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "",
    "^(next/(.*)$)|^(next$)",
    "",
    "^(@context|@cmp|@|@img)(/.*)$",
    "",
    "^\\.\\./.*$",
    "",
    "^[.]",
    "",
    ".*\\.css$"
  ]
}
